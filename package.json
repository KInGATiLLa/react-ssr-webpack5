{
  "name": "react-final-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:dev": "rimraf ./dist && webpack --mode=development --config webpack/dev/webpack.dev.server.js --config webpack/dev/webpack.dev.client.js && node dist/server.js",
    "start": "cross-env NODE_ENV=development npm-run-all --parallel startWatch:*",
    "startWatch:build-server": "webpack --config webpack/dev/webpack.dev.server.js --watch",
    "startWatch:build-client": "webpack --config webpack/dev/webpack.dev.client.js --watch --no-cache",
    "startWatch:server": "nodemon --watch dist --exec node dist/server.js",
    "build": "cross-env NODE_ENV=production rimraf ./dist && webpack --config webpack/prod/webpack.prod.server.js --config webpack/prod/webpack.prod.client.js",
    "build:dev": "rimraf ./dist && webpack --config webpack/dev/webpack.dev.server.js --config webpack/dev/webpack.dev.client.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
